<Window x:Class="web_print_agent.NotificationWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:web_print_agent"
        mc:Ignorable="d"
        ResizeMode="NoResize"
        WindowStyle="None"
        Title="NotificationWindow" Height="100" Width="300" Background="Transparent">
    <Grid Background="#A0333333">
        <Button Click="Button_Click" Content="Close" HorizontalAlignment="Left" Margin="225,0,0,0" VerticalAlignment="Top" Width="75" Visibility="Hidden"/>
        <TextBlock x:Name="tbTitle" HorizontalAlignment="Left" Margin="31,16,0,0" TextWrapping="Wrap" Text="TextBlock" VerticalAlignment="Top"  FontSize="18" Foreground="White"/>
        <TextBlock x:Name="tbContent" HorizontalAlignment="Left" Margin="31,58,0,0" TextWrapping="Wrap" Text="TextBlock" VerticalAlignment="Top" Foreground="White" />
    </Grid>
</Window>
